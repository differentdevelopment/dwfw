{
    "name": "different/dwfw",
    "description": "Different Development Web FrameWork",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Kukel Attila",
            "email": "kukel.attila@different.hu"
        }
    ],
    "keywords": [
        "framework",
        "laravel",
        "backpack",
        "different"
    ],
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^8.0",
        "backpack/crud": "5.6.1",
        "backpack/pro": "1.6.2",
        "backpack/filemanager": "*",
        "backpack/settings": "*",
        "fideloper/proxy": "^4.4",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^9.0",
        "laravel/ui": "^3.0",
        "lukeraymonddowning/honey": "*",
        "spatie/laravel-honeypot": "*",
        "spatie/laravel-permission": "*"
    },
    "require-dev": {
        "facade/ignition": "2.5.0",
        "fzaninotto/faker": "*",
        "laracasts/generators": "^2.0",
        "laravel/tinker": "^2.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.3"
    },
    "autoload": {
        "files": [
            "src/app/Helpers/helpers.php"
        ],
        "psr-4": {
            "Different\\Dwfw\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Different\\Dwfw\\Tests\\": "tests"
        },
        "exclude-from-classmap": [
            "**/config"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Different\\Dwfw\\DwfwServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan dwfw:upgrade --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        }
    ]
}
