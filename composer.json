{
    "name": "different/dwfw",
    "description": "Different Development Web FrameWork",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Kukel Attila",
            "email": "kukel.attila@different.hu"
        }
    ],
    "keywords": [
        "framework",
        "laravel",
        "backpack",
        "different"
    ],
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "backpack/crud": "4.1.*",
        "backpack/filemanager": "^1.1",
        "backpack/permissionmanager": "6.0.*",
        "backpack/settings": "3.0.*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "8.*",
        "laravel/ui": "^3.0",
        "lukeraymonddowning/honey": "^0.3.1",
        "spatie/laravel-honeypot": "^3.0"
    },
    "require-dev": {
        "backpack/generators": "^3.1",
        "facade/ignition": "^2.3",
        "fzaninotto/faker": "^1.9.1",
        "laracasts/generators": "^1.1",
        "laravel/tinker": "^2.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Different\\Dwfw\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Different\\Dwfw\\Tests\\": "tests"
        },
        "exclude-from-classmap": [
            "**/config"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Different\\Dwfw\\DwfwServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan dwfw:upgrade --ansi"
        ]
    }
}
